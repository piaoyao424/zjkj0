<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppTheme" parent="android:Theme.Light" />
    <!-- login & regist -->
    <style name="login_regist_editlayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@drawable/login_edittext_bg</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="login_regist_textview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@null</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#406080</item>
    </style>

    <style name="login_regist_editview">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:background">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="login_regist_imageseperator">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:background">@drawable/vertical_blue_line</item>
    </style>

    <!-- HomePage -->
    <style name="homeview_line_linearlayout">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="homeview_icon_linearlayout">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@null</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="homeview_icon_imagebutton">
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:background">@null</item>
    </style>

    <style name="homeview_icon_textview">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:background">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <!-- tab -->
    <style name="tab_linear_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>

    <style name="tab_image_style">
        <item name="android:focusable">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:gravity">center_horizontal|bottom</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:paddingTop">5dp</item>
    </style>

    <style name="tab_textview_style">
        <item name="android:layout_gravity">center_horizontal|bottom</item>
        <item name="android:gravity">top|center_horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12dp</item>
        <item name="android:textColor">@color/tab_gray</item>
        <item name="android:background">@null</item>
        <item name="android:paddingBottom">2dp</item>
    </style>

    <!-- basicinfo -->
    <style name="basicinfo_left_text">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/main_text_color</item>
    </style>

    <style name="basicinfo_right_text">
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/main_text_color</item>
    </style>

    <style name="basicinfo_relative_above">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>

    <style name="basicinfo_relative_below">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:layout_marginRight">1dp</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
    </style>

    <style name="basicinfo_button">
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:background">@drawable/selector_driver_grab</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <!-- goal -->
    <style name="mygoal_left_text">
        <item name="android:textSize">11dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:textColor">@color/main_text_color</item>
    </style>

    <style name="mygoal_right_text">
        <item name="android:textSize">11dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:textColor">@color/main_text_color</item>
    </style>

    <style name="mygoal_horizontal_separator">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@drawable/my_goal_gray_bg</item>
    </style>

    <style name="mygoal_vertical_separator">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@drawable/my_goal_gray_bg</item>
    </style>

    <style name="mygoal_time_left">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">13sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="date_button_style">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">13sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:padding">2dp</item>
        <item name="android:hint">选择日期</item>
        <item name="android:background">@null</item>
    </style>
    <!-- consume -->
    <style name="myconsume_text">
        <item name="android:textSize">14sp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="myconsume_list_text">
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/main_text_color</item>
    </style>
    <!-- goal_main -->
    <style name="goal_linear">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="goal_text">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/title_selected_gray</item>
        
    </style>

    <style name="goal_separator">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">4dp</item>
        <item name="android:background">@drawable/goal_topbar_normal</item>
    </style>

    <!-- joinshop -->
    <style name="myrating_style">
        <item name="android:progressDrawable">@drawable/my_ratingbar_star</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">21dp</item>
    </style>

    <style name="shop_dertail_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@drawable/my_goal_gray_bg</item>
    </style>

    <!-- aboutus -->
    <style name="aboutus_relative_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@drawable/my_goal_white_bg</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="aboutus_text_style">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="aboutus_contactus_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>

    <style name="aboutus_image_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/my_consume_arrow</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>
    <!-- ListRadioButton -->
    <style name="list_radio_button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:button">@drawable/selector_radiobutton</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:paddingLeft">30px</item>
        <item name="android:singleLine">true</item>
    </style>
    <!-- callview_edit -->
    <style name="calltaxi_center_editlayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@drawable/login_edittext_bg</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="calltaxi_grab_editlayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@drawable/login_edittext_bg</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="aboutus_guide_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:background">@null</item>
    </style>

    <style name="double_type_text">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/title_selected_gray</item>
    </style>
    <!-- screenlock calltaxi -->
    <style name="screen_lock_result_texttitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:gravity">center_vertical|right</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@null</item>
    </style>

    <style name="screen_lock_result_textcontent">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@null</item>
    </style>

    <!-- order grab show -->
    <style name="grab_order_textlayout">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">4</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <!-- Jump Window -->
    <style name="MyTheme_Transparent" parent="@android:style/Theme.Translucent">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
	
    <style name="Designate_list_text_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@null</item>
        <item name="android:gravity">center_vertical</item>
        </style>
</resources>