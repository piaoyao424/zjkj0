<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape
          xmlns:android="http://schemas.android.com/apk/res/android">
            <gradient android:startColor="#fff7a600" android:endColor="#ffffc700" android:angle="270.0" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape
          xmlns:android="http://schemas.android.com/apk/res/android">
            <gradient android:startColor="#fff7a600" android:endColor="#ffffc700" android:angle="270.0" />
        </shape>
    </item>
    <item android:state_pressed="false" android:drawable="@drawable/listbg2" />
</selector>
